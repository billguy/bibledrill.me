.middlePage
  .panel.panel-info
    .panel-heading
      h3.panel-title Please Sign In
    .panel-body
      .row
        .col-md-5
          a href="#{omniauth_authorize_path(resource_name, :facebook)}"
            img width='250' height='53' alt='fb' src='data:image/png;base64,'
          br
          br
        .col-md-7 style=("border-left:1px solid #ccc;")

          = form_for(resource, as: resource_name, url: session_path(resource_name), html_options: {class: 'form-horizontal'}) do |f|
            fieldset
              = f.email_field :email, autofocus: true, class: 'form-control input-md', placeholder: 'Email'
              - if devise_mapping.rememberable?
                .spacing
                  = f.check_box :remember_me
                  small< Remember me
              = f.password_field :password, autocomplete: "off", class: 'form-control input-md', placeholder: 'Password'
              .spacing
                = render "devise/shared/links"
                br
              button#singlebutton.btn.btn-info.btn-sm.pull-right name="singlebutton" Sign In